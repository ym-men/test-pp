.container
  width: 300px;
  height: auto;
  border-radius: 2px;
  border: dashed 1px rgba(0, 0, 0, 0.3);
  padding: 0px 20px 35px 20px;
  margin-top: 20px;
  text-align: center;
  align-items: center;

.headTitle
  margin-top: 20px;
  font-size: 24px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: normal;
  letter-spacing: normal;
  color: Basic1000;

.text
  margin: 10px 0px 10px 0px;
  font-size: 16px;
  font-weight: normal;
  font-style: normal;
  font-stretch: normal;
  line-height: 1.29;
  letter-spacing: normal;

.oval
  margin-top: 30px;
  width: 80px;
  height : 80px;
  border-radius: 50%;
  background-color: Basic600;

.icon
  width: 30px;
  height : 30px;

.clockIcon {
  width: 40px;
  height: 40px;
  margin: 4px 0px 0px 4px;
}

.checkBox {
  label {
    align-items: start;
  }
}